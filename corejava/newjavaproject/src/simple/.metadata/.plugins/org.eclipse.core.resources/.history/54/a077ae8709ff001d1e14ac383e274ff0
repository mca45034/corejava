package Strings;

public class ReplceSymbole {
	public static void main(String[] args) {
		String s="rahul kirar";
		char[] ch=s.toCharArray();
		for(int i=0;i<ch.length;i++) {
			if(ch[i]=='r'||ch[i]=='u'||ch[i]=='k') {
				ch[i]='$';
			}
		}
		for(int i=0;i<ch.length;i++) {
			System.out.println(ch[i]);
		}
	}
}
