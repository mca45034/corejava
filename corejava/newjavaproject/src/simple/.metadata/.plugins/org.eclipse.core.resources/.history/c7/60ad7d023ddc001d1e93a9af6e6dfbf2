package Question_Prectice;

public class FrequencyOfArry {

	public static void main(String[] args) {
		int a[]= {1,1,2,3,3,4,5,5,6,6};
		for(int i=0;i<a.length;i++) {
			int count = 1;
			for(int j=i+1;i<a.length;i++) {
				if(a[i] == a[j]) {
					
					count++;
			a[j]--;
				}
				
			}
			System.out.println(a[i]+"      "+count);
		}

	}

}
