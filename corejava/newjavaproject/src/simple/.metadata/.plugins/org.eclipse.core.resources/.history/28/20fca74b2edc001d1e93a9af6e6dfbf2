package Question_Prectice;

public class LeftRoteted {
	public static void main(String[] args) {
		int arr[]= {9,2,3,4,5,6,7,8};
		int n=8;
		for(int i=0;i<n;i++) {
			int f=arr[0];
			int j;
			for( j=0;j<arr.length-1;j--) {
				arr[j]=arr[j+1];
			}
			arr[j]=f;
			
		}
		for(int i=0;i<arr.length;i++)
				{
			System.out.print(arr[i]);
				}
		}
	}


