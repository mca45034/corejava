package in.com.String;

public class CountCharacter {

	public static void main(String[] args) {
		String name="malayalam";
		char ch[]=name.toCharArray();
		int count=0;
		for (int i = 1; i < ch.length; i++) {
			if(ch.equals('a')) {
				count=ch[i];
				count++;
			}
			
		}
		System.out.println(count);

	}

}
