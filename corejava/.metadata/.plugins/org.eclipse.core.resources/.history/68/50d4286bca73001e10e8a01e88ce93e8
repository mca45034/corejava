package in.com.Aggrigation_AComparator_ArrayList;

public class Book {
	int id;
	String name;
	int price;
	public Book(int id, String name, int price) {
		super();
		this.id = id;
		this.name = name;
		this.price = price;
	}
	@Override
	public String toString() {
		return "Book [id=" + id + ", name=" + name + ", price=" + price + "]";
	}
	

}
