		hm.put("shubh", 888);
		hm.put("alex", 333);
		Collection<Integer>cl=hm.values();
		Iterator<Integer>it=cl.iterator();
		while(it.hasNext()) {
			Integer i=it.next();
			al.add(i);
			
		}
