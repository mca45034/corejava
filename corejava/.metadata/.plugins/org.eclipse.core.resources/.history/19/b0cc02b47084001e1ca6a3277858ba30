package NewOne;

public class Marksheet {
	private int id;
	private String name;
	private int rollNo;
	private int pysics;
	private int chemistry;
	private int maths;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getRollNo() {
		return rollNo;
	}
	public void setRollNo(int rollNo) {
		this.rollNo = rollNo;
	}
	public int getPysics() {
		return pysics;
	}
	public void setPysics(int pysics) {
		this.pysics = pysics;
	}
	public int getChemistry() {
		return chemistry;
	}
	public void setChemistry(int chemistry) {
		this.chemistry = chemistry;
	}
	public int getMaths() {
		return maths;
	}
	public void setMaths(int math) {
		this.maths = maths;
	}
	

}
