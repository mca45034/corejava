package in.com.ArrayList_1;

import java.util.ArrayList;

class Employe {
	int id;
	String name;
	int salary;
	public Employe(int id, String name, int salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
	@Override
	public String toString() {
		return "Employe123 [id=" + id + ", name=" + name + ", salary=" + salary + "]";
	}

}
public class Employe123{
	public static void main(String[] args) {
	ArrayList<Employe>al=new ArrayList<Employe>();
	al.add(new Employe(101, "sham", 10000);
	al.add(new Employe(901, "ram", 11000);
	al.add(new Employe(99, "sachin", 9000);
	al.add(new Employe(202, "pappu", 10000);
	al.add(new Employe(98, "bhalu", 10000);
		
	}
}
 