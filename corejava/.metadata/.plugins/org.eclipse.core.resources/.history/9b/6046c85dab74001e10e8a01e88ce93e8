package in.com.Thread.Yield;

public class Yield_main implements Runnable{
	@Override
	public void run() {
		String s=Thread.currentThread().getName();
		for (int i = 0; i < 10; i++) {
			System.out.println(s+" "+i);
			Thread.yield();
		}
		
	}

	public static void main(String[] args) {
		

	
}

	
}