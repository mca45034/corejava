
package in.com.Basic;

public class Polindram1 {

	public static void main(String[] args) {
		int num=10000;
		int sum=0;
		int tmp=num;
		for (int i = 1000; i <num; i++) {
			for (int j = i+1; j < num; j++) {
		if(i%j==0) {
			int rem=num%10;
			sum=sum*10+rem;
			num=num/10;
		}
		}
			System.out.println(sum);
		}
//		if(tmp==sum) {
//			System.out.println("true");
//		}
//		else {
//			System.out.println("false");
//		}

	}

}
