package in.com.OI;

import java.io.File;

public class Delete_File {

	public static void main(String[] args) {
		File myfile=new File("rahulk.java");
		if(myfile.delete()) {
			System.out.println("Delet file are"+  myfile.getName());
		}
		else {
			System.out.println("not delet");
		}
		System.out.println("done");

	}

}
