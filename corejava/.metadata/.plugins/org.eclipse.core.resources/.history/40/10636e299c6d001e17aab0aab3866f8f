package in.com.OI;

import java.io.File;
import java.io.IOException;

public class UserTextAndRead_Write {

	public static void main(String[] args) {
		File myfile=new File("file.java");
		try {
			myfile.createNewFile();
		} catch (IOException e) {
			System.out.println("file not found...");
			e.printStackTrace();
		}
		
		

	}

}
