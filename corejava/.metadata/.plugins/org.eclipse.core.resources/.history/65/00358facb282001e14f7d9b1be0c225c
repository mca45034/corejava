package DaemonS;
class Student{
	int id;String name;int age;


	public Student(int id, String name, int age) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
	}
	public void disp(){
		System.out.println("Student id is.."+id);
		System.out.println("student name is.."+name);
		System.out.println("student age is.."+age);
	}
	
}
